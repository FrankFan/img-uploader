{
  "name": "image uploader",
  "description": "Image Uploader using cnblogs img service",
  "version": "1.1",
  "manifest_version": 2,
  "author": "Frank Fan",
  "homepage_url": "https://github.com/FrankFan/img-uploader",

  "background": {
    "persistent": false,
    "page": "background.html"
  },

  "browser_action": {
    "default_icon": "assets/128.png"
  },

  "icons": {
    "16": "assets/32.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "128": "assets/128.png",
    "256": "assets/256.png"
  },
  
  "permissions": [
    "*://*.cnblogs.com/"
   ]
}