{
  "name": "image uploader",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Image Uploader using cnblogs img service",

  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  
  "icons": {
    "16": "assets/32.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "128": "assets/128.png",
    "256": "assets/256.png"
  },

  "permissions": [
    "unlimitedStorage", "contextMenus", "tabs"
  ]
}